{
    "name": "iamgerwin/laravel-api-scaffold",
    "description": "A Laravel package to scaffold Service layer architecture with API resources, controllers, and tests",
    "keywords": [
        "laravel",
        "service-layer",
        "api",
        "scaffold",
        "artisan",
        "architecture",
        "iamgerwin"
    ],
    "homepage": "https://github.com/iamgerwin/laravel-api-scaffold",
    "license": "MIT",
    "authors": [
        {
            "name": "iamgerwin",
            "email": "iamgerwin@live.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1|^8.2|^8.3|^8.4",
        "illuminate/contracts": "^10.0|^11.0|^12.0",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "laravel/prompts": "^0.1.0|^0.3.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^7.0|^8.0|^9.0",
        "orchestra/testbench": "^8.0|^9.0|^10.0",
        "pestphp/pest": "^2.0|^3.0",
        "pestphp/pest-plugin-laravel": "^2.0|^3.0",
        "phpstan/phpstan": "^1.10|^2.0",
        "phpunit/phpunit": "^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Iamgerwin\\LaravelApiScaffold\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Iamgerwin\\LaravelApiScaffold\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint",
        "analyse": "vendor/bin/phpstan analyse"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Iamgerwin\\LaravelApiScaffold\\LaravelApiScaffoldServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
